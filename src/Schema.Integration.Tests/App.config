<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <appSettings>
    <add key="Wrapper.ConsumerKey" value="YOUR_KEY_HERE" />
    <add key="Wrapper.ConsumerSecret" value="YOUR_SECRET_HERE" />
    <add key="Wrapper.BaseUrl" value="http://api.7digital.com/1.2" />
    <add key="Wrapper.SecureBaseUrl" value="https://api.7digital.com/1.2" />

    <!-- NOTE: These will need to be filled in for the 3 legged oauth tests to pass -->
    <add key="Integration.Tests.AccessToken" value="" />
    <add key="Integration.Tests.AccessTokenSecret" value="" />

    <!-- NOTE: These will need to be filled in for Purchasing tests to pass -->
    <add key="Wrapper.Purchasing.ConsumerKey" value="YOUR_KEY_HERE" />
    <add key="Wrapper.Purchasing.ConsumerSecret" value="YOUR_SECRET_HERE" />
    <add key="Integration.Tests.Purchasing.AccessToken" value="" />
    <add key="Integration.Tests.Purchasing.AccessTokenSecret" value="" />
    <add key="Integration.Tests.Purchasing.ReleaseId" value="" />
  </appSettings>
  <startup>
  <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5.1" /></startup>
  <runtime>
    <assemblyBinding xmlns="urn:schemas-microsoft-com:asm.v1">
      <dependentAssembly>
        <assemblyIdentity name="Newtonsoft.Json" publicKeyToken="30ad4fe6b2a6aeed" culture="neutral" />
        <bindingRedirect oldVersion="0.0.0.0-10.0.0.0" newVersion="10.0.0.0" />
      </dependentAssembly>
    </assemblyBinding>
  </runtime>
